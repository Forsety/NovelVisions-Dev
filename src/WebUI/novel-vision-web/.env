# .env
# Environment variables for Frontend

# API URLs
REACT_APP_CATALOG_API_URL=https://localhost:7295
REACT_APP_GATEWAY_URL=http://localhost:5000

# Disable SSL verification for development (self-signed certificate)
HTTPS=false
SSL_CRT_FILE=
SSL_KEY_FILE=

# For axios to work with self-signed certificates
NODE_TLS_REJECT_UNAUTHORIZED=0